{
  "name": "med-reminder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:next dev:scss",
    "dev:next": "cross-env NODE_ENV=development nodemon",
    "dev:scss": "typed-scss-modules \"src/**/*.scss\" -w",
    "typegen:scss": "typed-scss-modules \"src/**/*.scss\"",
    "build": "run-p build:next build:server",
    "build:next": "prisma generate && prisma db push && next build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node --max-old-space-size=512 -r ts-node/register -r tsconfig-paths/register src/server/index.ts",
    "lint": "next lint"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/material-nextjs": "^5.15.7",
    "@mui/x-date-pickers": "^6.19.5",
    "@prisma/client": "^5.11.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@unocss/reset": "^0.58.3",
    "cloudinary": "^2.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "next": "^14.1.4",
    "next-auth": "^4.24.7",
    "node-schedule": "^2.1.1",
    "react": "^18.2.0",
    "react-calendar": "^4.8.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.1",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.4",
    "react-zoom-pan-pinch": "^3.4.3",
    "sharp": "^0.33.2",
    "swiper": "^11.0.7",
    "uuid": "^9.0.1",
    "vaul": "^0.9.0",
    "web-push": "^3.6.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/node-schedule": "^2.1.6",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "sass": "^1.70.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typed-scss-modules": "8.0.0",
    "typescript": "^5.4.2"
  }
}
